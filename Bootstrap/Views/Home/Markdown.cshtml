@{
    ViewBag.Title = "Markdown";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("~/Content/Prettify/Themes/sons-of-obsidian.css")" rel="stylesheet" type="text/css" />

<script type="text/javascript">
    $(function () {

        $('pre').addClass('prettyprint linenums');

        prettyPrint();
    });
</script>

<h2 class="page-header">Markdown</h2>

@Html.Raw(ViewData["content"].ToString())
<p>@Html.ActionLink("Edit this Page", "Edit")</p>

